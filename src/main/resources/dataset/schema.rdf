<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:osgb="http://www.ordnancesurvey.co.uk/xml/namespaces/osgb"
         xmlns:gml="http://www.opengis.net/ont/gml#"
         xmlns:geo="http://www.opengis.net/ont/geosparql#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:ntu="http://ntu.ac.uk/ont/geo#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
   
   <rdfs:Class rdf:about="ntu:RoadNode">
      <rdfs:subClassOf rdf:resource="geo:Feature"/>
      <rdfs:subClassOf rdf:resource="osgb:RoadNode"/>
   </rdfs:Class>
   <rdfs:Class rdf:about="ntu:RoadLink">
      <rdfs:subClassOf rdf:resource="geo:Feature"/>
      <rdfs:subClassOf rdf:resource="osgb:RoadLink"/>
   </rdfs:Class>
   <rdfs:Class rdf:about="ntu:TopographicArea">
      <rdfs:subClassOf rdf:resource="geo:Feature"/>
      <rdfs:subClassOf rdf:resource="osgb:TopographicArea"/>
   </rdfs:Class>
   <rdf:Property rdf:about="ntu:hasExactGeometry">
      <rdfs:subPropertyOf rdf:resource="geo:hasGeometry"/>
      <rdfs:subPropertyOf rdf:resource="geo:hasDefaultGeometry"/>
   </rdf:Property>
   <rdf:Property rdf:about="ntu:hasPointGeometry">
      <rdfs:subPropertyOf rdf:resource="geo:hasGeometry"/>
      <rdfs:subPropertyOf rdf:resource="geo:hasDefaultGeometry"/>
   </rdf:Property>
   <geo:gmlLiteral rdf:about="rdfs:Datatype">
      <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/spec/geosparql/1.0"/>
      <rdfs:label xml:lang="en">GML literal</rdfs:label>
      <rdfs:comment xml:lang="en">The datatype of GML literal values</rdfs:comment>
   </geo:gmlLiteral>      
</rdf:RDF>
