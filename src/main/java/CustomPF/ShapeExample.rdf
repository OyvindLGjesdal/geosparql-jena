<?xml version="1.0" encoding="UTF-8"?>

<! Example Gml Literal for Point >
<gml:Point gml:id="ID01" srsName="http://www.opengis.net/def/crs/EPSG/0/4326">
    <gml:pos>45.256 -71.92</gml:pos>
</gml:Point>

<! Example Gml Literal for Line String >
<gml:LineString gml:id="ID02" srsName="http://www.opengis.net/def/crs/EPSG/0/4326">
    <gml:posList>
        45.256 -110.45 46.46 -109.48 43.84 -109.86
    </gml:posList>
</gml:LineString>

<! Example Gml Literal for Polygon >
<gml:Polygon gml:id="ID003" srsName="http://www.opengis.net/def/crs/EPSG/0/4326">
    <gml:exterior>
        <gml:LinearRing>
           <gml:posList>
        45.256 -110.45 46.46 -109.48 43.84 -109.86 45.256 -110.45
           </gml:posList>
        </gml:LinearRing>
    </gml:exterior>
</gml:Polygon>